{
  "expo": {
    "name": "भ ओ जन Delivery",
    "slug": "bhaojan-delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-logo.png",
    "scheme": "bhojandelivery",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/app-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDF8F3"
    },
    "android": {
      "package": "com.bhaojandelivery.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-logo.png",
        "backgroundColor": "#FDF8F3"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bhojandelivery.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/app-logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bfe669fe-d044-4cff-8ee0-653d2be28961"
      }
    }
  }
}
