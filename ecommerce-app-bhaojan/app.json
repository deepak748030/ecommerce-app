{
  "expo": {
    "name": "The Art Of भ ओ जन",
    "slug": "the-art-of-bhojan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-logo.png",
    "scheme": "artofbhojan",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/app-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDF8F3"
    },
    "android": {
      "package": "com.artofbhojan.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-logo.png",
        "backgroundColor": "#FDF8F3"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/app-logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2dfaed95-d77f-4c5a-aaf9-e41b9367bb6a"
      }
    }
  }
}
